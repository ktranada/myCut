<!DOCTYPE html>
<html>
  <head>
    <title>myCut</title>

    <% if Rails.env.production? %>
      <% api_key = "ENV['google_maps_js_key']" %>
    <% else %>
      <% api_key = '' %>
    <% end %>
    <script src="//use.typekit.net/tmx1xsr.js"></script>

    <script>try{Typekit.load();}catch(e){}</script>
    <link href='http://fonts.googleapis.com/css?family=Oswald:400,300,700' rel='stylesheet' type='text/css'>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=places,key=<%= api_key %>"></script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>

    <script type="text/javascript">
      window.BARBER_CLOUDINARY = {
        "cloud_name": "<%= ENV['CLOUD_NAME'] %>",
        "upload_preset": "<%= ENV['BARBER_PRESET'] %>"
      };
      window.USER_CLOUDINARY = {
        "cloud_name": "<%= ENV['CLOUD_NAME'] %>",
        "upload_preset": "<%= ENV['AVATAR_UPLOAD_PRESET'] %>"
      };
      window.BARBER_PORT_CLOUDINARY = {
        "cloud_name": "<%= ENV['CLOUD_NAME'] %>",
        "upload_preset": "<%= ENV['BARBER_PICS_PRESET'] %>"
      };
    </script>

  </head>
  <body>



    <%= yield %>

    <!-- <% if signed_in? %>
      <%= render 'layouts/footer' %>
    <% end %> -->

    <!-- < script src="assets/bootstrap-tagsinput.min.js">< /script>
    < link rel="stylesheet" href="assets/bootstrap-tagsinput.css"> -->


  </body>
</html>
