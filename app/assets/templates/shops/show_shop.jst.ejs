<div class="container-fluid level-two">
  <div class="row feed-header">
    <div class="col-md-7 col-xs-6 col-md-offset-1">
      <h1><em><%= shop.escape('name') %></em>
        <p class="lead"><div class="rating" data-path="assets/images"></div></p>
      </h1>
    </div>
    <div class="shop-feature col-md-2">
    <br>
        <a href="#/shops/<%= shop.escape('id') %>/review/new"><button class="btn btn-default">WRITE A REVIEW</button></a>
        <br><br>
        <a href="#"><button style="color: crimson;" class="btn btn-default">ALL SHOPS</button></a>
    </div>
  </div>
  <br><br><br>

  <div class="row shop-barbers-photos">
    <div class="col-xs-9 col-md-9 col-md-offset-3">
      <% barbers.each(function(barber) { %>
        <div class="barber-photo col-xs-3 col-md-4"><a href="/#/shops/<%= shop.escape('id') %>/barbers/<%= barber.escape('barber_id') %>"><img class="barber-profile" src="<%= barber.escape('photo_url') %>"></a></div>
      <% }); %>
    </div>
  </div>
  <br><br>
  <div class="row level-three">
    <div class="reviews-index">
      <ul class="review-items col-xs-12 col-md-6 col-md-offset-3">
      </ul>
    </div>
  </div>
  <br>
</div>
